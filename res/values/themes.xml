<resources>

	<drawable name="transparent_background">#66000000</drawable>
	<style name="ServiceShowWarning" parent="@android:style/Theme.Translucent">
		<item name="android:windowBackground">@drawable/transparent_background</item>
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
	</style>
	
	<style name="Theme.Home" parent="@android:style/Theme.Black">
		<item name="android:attr/buttonStyle">@style/Custom.Button.Home</item>
		<item name="android:attr/imageButtonStyle">@style/Custom.ImageButton.Home</item>
		<item name="android:attr/textViewStyle">@style/Custom.TextView.Home</item>
	</style>

</resources>
